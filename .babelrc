{
  "presets": [
    ["@babel/preset-react"],
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "transform-class-properties",
    "react-hot-loader/babel"
  ]
}
